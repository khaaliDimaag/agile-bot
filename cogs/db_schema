[DB] `khaaliDiscord`
[Collection] members: {
  discord_id: int,
  name: str,
  handle: str,
  created: date,
  bot: boolean,
  guilds: {
    guild_id: {
      nick: str,
      roles: [],
      joined: date,
      left: date
    }
  }
}
[Collection] channels: {
  name: str,
  discord_id: int,
  created: date,
  guild_id: int,
  voice: boolean,
  category: int
}
[Collection] roles: {
  name: str,
  discord_id: int,
  value: int,
  perms: [],
  created: date,
  guild_id: int,
  managed: boolean
}
[Collection] webhooks: {
  discord_id: int,
  name: str,
  token: str,
  url: str,
  created: date,
  guild_id: int
}
[Collection] guilds: {
  name: str,
  discord_id: int,
  owner_id: int,
  created: date,
  features: [],
  stats_chan_id: int,
  Member: [],
  Channel: [],
  Role: [],
  Webhook: []
}

[DB] khaaliProjects:
[Collection] projects: {
  name: str,
  repo: url,
  start: date,
  release: str,
  product_backlog: [],
  release_backlog: [],
  sprints: []
}
[Collection] stories: {
  inshort: str,
  details: str,
  project: ObjectID,
  parents: [],
  children: [],
  added: date,
  timereq: int,
  timeleft: int
}
[Collection] sprints: {
  project: ObjectID,
  number: int,
  start: date,
  end: date,
  stories: [],
  incomplete: [],
  review: str
}