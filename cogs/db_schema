[DB] khaaliProjects:
[Collection] projects: {
  name: str,
  repo: url,
  start: date,
  release: str,
  product_backlog: [],
  release_backlog: [],
  sprints: []
}
[Collection] stories: {
  inshort: str,
  details: str,
  project: ObjectID,
  parents: [],
  children: [],
  added: date,
  timereq: int,
  timeleft: int
}
[Collection] sprints: {
  project: ObjectID,
  number: int,
  start: date,
  end: date,
  stories: [],
  incomplete: [],
  review: str
}